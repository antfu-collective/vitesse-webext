{
  "name": "vitesse-webext",
  "displayName": "Vitesse WebExt",
  "version": "0.0.1",
  "description": "[description]",
  "private": true,
  "scripts": {
    "dev": "npm run clear && cross-env NODE_ENV=development run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite",
    "dev:js": "npm run build:js -- --mode development",
    "build": "cross-env NODE_ENV=production run-s clear build:web build:prepare build:js",
    "build:prepare": "esno scripts/prepare.ts",
    "build:web": "vite build",
    "build:js": "vite build --config vite.config.content.ts",
    "pack": "crx pack extension -o ./extension.crx",
    "clear": "rimraf extension/dist extension/manifest.json",
    "lint": "eslint '**/*.{json,ts,js,vue}'"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.7.0",
    "@iconify/json": "^1.1.395",
    "@types/fs-extra": "^9.0.12",
    "@types/node": "^16.7.6",
    "@types/webextension-polyfill": "^0.8.0",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@vitejs/plugin-vue": "^1.6.0",
    "@vue/compiler-sfc": "^3.2.6",
    "@vueuse/core": "^6.0.0",
    "chokidar": "^3.5.2",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "eslint": "^7.32.0",
    "esno": "^0.9.1",
    "fs-extra": "^10.0.0",
    "kolorist": "^1.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.2",
    "unplugin-auto-import": "^0.2.4",
    "unplugin-icons": "^0.7.3",
    "unplugin-vue-components": "^0.14.5",
    "vite": "^2.5.1",
    "vite-plugin-windicss": "^1.3.0",
    "vue": "^3.2.6",
    "vue-demi": "^0.11.3",
    "webext-bridge": "^4.1.1",
    "webextension-polyfill": "^0.8.0"
  }
}
